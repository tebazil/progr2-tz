{% extends 'base.html.twig' %}

{% block title %}Дом{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
</style>

<div class="example-wrapper">
    <h1>Дом</h1>
    <form method="post">
        <input type="hidden" name="token" value="{{ csrf_token('token') }}"/>
        Улица : <input name="streetName" value="{{ street_name }}"><br>
        Дом : <input name="number" value="{{ number }}"><br>
        <input type="submit">
    </form>
</div>
{% endblock %}
